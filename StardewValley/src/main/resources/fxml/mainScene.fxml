<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.image.ImageView?>

<StackPane fx:id="rootVBox" alignment="CENTER" xmlns:fx="http://javafx.com/fxml" fx:controller="stardewValley.screens.MainScene">
  <VBox alignment="CENTER" spacing="20.0">
    <padding>
      <Insets bottom="40.0" left="40.0" right="40.0" top="40.0"/>
    </padding>
  </VBox>
  <ImageView fx:id="backgroundImage" preserveRatio="true" StackPane.alignment="CENTER"/>
  <Label fx:id="pressEnterLabel" text="Press Enter to continue" StackPane.alignment="BOTTOM_CENTER" styleClass="pixel-font"/>
</StackPane>
